# AKHAN Development Environment
ENVIRONMENT_NAME=Development
ENVIRONMENT_TYPE=dev

# Environment-specific URLs
BASE_URL=https://akhan-dev.americas.inet.net
API_BASE_URL=https://api-akhan-dev.americas.inet.net
ADMIN_URL=https://admin-akhan-dev.americas.inet.net
DB_HOST=akhan-dev-db.americas.cloud.com

# Dev-specific settings
HEADLESS=false
DEBUG_MODE=true
LOG_LEVEL=debug
BROWSER_DEVTOOLS=true
BROWSER_SLOWMO=0

# Dev feature flags
FEATURE_FLAGS=newUI:true;betaFeatures:true;aiAssist:true;debugPanel:true

# Dev test data
TEST_USER_PREFIX=dev_test_
TEST_DATA_CLEANUP=true

# Dev-specific timeouts (more lenient)
DEFAULT_TIMEOUT=60000
BROWSER_ACTION_TIMEOUT=15000