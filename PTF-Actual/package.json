{
  "name": "@mdakhan.mak/cs-playwright-test-framework",
  "version": "1.5.16",
  "description": "CS Playwright Test Framework - Lightning-fast, configurable, AI-powered self-healing test automation with Playwright",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": "./dist/lib/index.js",
    "./ai": "./dist/lib/ai.js",
    "./api": "./dist/lib/api.js",
    "./assertions": "./dist/lib/assertions.js",
    "./bdd": "./dist/lib/bdd.js",
    "./browser": "./dist/lib/browser.js",
    "./core": "./dist/lib/core.js",
    "./data": "./dist/lib/data.js",
    "./database": "./dist/lib/database.js",
    "./element": "./dist/lib/element.js",
    "./reporter": "./dist/lib/reporter.js",
    "./utils": "./dist/lib/utils.js",
    "./package.json": "./package.json",
    "./*": "./dist/*"
  },
  "bin": {
    "cs-playwright-test": "dist/index.js",
    "cs-playwright-run": "dist/index.js",
    "cs-playwright-framework": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node scripts/post-build.js",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "postinstall": "playwright install",
    "test": "node dist/index.js",
    "test:zero-code": "node dist/index.js --project=orangehrm --tags=\"@zero-code and @smoke\"",
    "test:validation": "node dist/index.js --project=orangehrm --tags=@framework-validation",
    "test:critical": "node dist/index.js --project=orangehrm --tags=@TC902"
  },
  "keywords": [
    "test-automation",
    "testing-framework",
    "playwright",
    "cucumber",
    "bdd",
    "api-testing",
    "soap-testing",
    "database-testing",
    "self-healing",
    "ai-testing",
    "azure-devops",
    "test-framework",
    "e2e-testing",
    "integration-testing"
  ],
  "author": "CS Team",
  "license": "MIT",
  "dependencies": {
    "@cucumber/cucumber": "^10.0.0",
    "@cucumber/gherkin": "^27.0.0",
    "@cucumber/messages": "^24.0.0",
    "@playwright/test": "^1.56.0",
    "@types/xml2js": "^0.4.14",
    "archiver": "^7.0.0",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "glob": "^11.0.0",
    "https-proxy-agent": "^7.0.2",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "minimist": "^1.2.8",
    "mongodb": "^6.0.0",
    "mssql": "^12.0.0",
    "mysql2": "^3.6.0",
    "oracledb": "^6.0.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.1",
    "socks-proxy-agent": "^8.0.2",
    "ws": "^8.14.2",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/archiver": "^6.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/minimist": "^1.2.5",
    "@types/mssql": "^9.1.8",
    "@types/node": "^20.19.19",
    "@types/oracledb": "^6.0.0",
    "@types/pg": "^8.10.0",
    "@types/ws": "^8.5.10",
    "ajv": "^8.17.1",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/mdakhan/myproject/_git/ptf-ado-repo"
  },
  "publishConfig": {
    "registry": "https://pkgs.dev.azure.com/mdakhan/_packaging/cs-framework/npm/registry/"
  }
}
