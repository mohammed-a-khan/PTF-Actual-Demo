# AKHAN Staging Environment
ENVIRONMENT_NAME=Staging
ENVIRONMENT_TYPE=staging

# Environment-specific URLs
BASE_URL=https://akhan-staging.americas.inet.net
API_BASE_URL=https://api-akhan-staging.americas.inet.net
ADMIN_URL=https://admin-akhan-staging.americas.inet.net
DB_HOST=akhan-staging-db.americas.cloud.com

# Staging-specific settings
HEADLESS=true
DEBUG_MODE=false
LOG_LEVEL=info
BROWSER_DEVTOOLS=false
BROWSER_SLOWMO=0

# Staging feature flags
FEATURE_FLAGS=newUI:true;betaFeatures:true;aiAssist:false;debugPanel:false

# Staging test data
TEST_USER_PREFIX=staging_test_
TEST_DATA_CLEANUP=true

# Staging-specific timeouts
DEFAULT_TIMEOUT=45000
BROWSER_ACTION_TIMEOUT=10000

# Parallel execution for staging
DEFAULT_PARALLEL=true
DEFAULT_WORKERS=4